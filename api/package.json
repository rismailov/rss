{
    "name": "express-api-starter-ts",
    "description": "A minimal starter for an Express.js API with Typescript & Prisma ORM",
    "version": "1.2.0",
    "main": "src/index.ts",
    "scripts": {
        "start": "ts-node src/index.ts",
        "dev": "nodemon src/index.ts",
        "build": "tsc",
        "start:dist": "node dist/src/index.js",
        "lint": "eslint --fix \"src/**/*.{ts,tsx}\"",
        "typecheck": "tsc --noEmit",
        "format": "pretty-quick --staged",
        "prepare": "cd .. && husky api/.husky"
    },
    "keywords": [],
    "license": "MIT",
    "dependencies": {
        "@prisma/client": "^5.14.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "helmet": "^7.1.0",
        "morgan": "^1.10.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.12.12",
        "@typescript-eslint/eslint-plugin": "^7.10.0",
        "@typescript-eslint/parser": "^7.10.0",
        "eslint": "^8.57.0",
        "eslint-config-airbnb-typescript": "^18.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "husky": "^9.0.11",
        "nodemon": "^3.1.0",
        "prettier": "^3.2.5",
        "pretty-quick": "^4.0.0",
        "prisma": "^5.14.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5",
        "zod": "^3.23.8"
    }
}
